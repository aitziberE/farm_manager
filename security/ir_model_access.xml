<?xml version="1.0" encoding="utf-8"?>

<odoo>

    <record model="ir.model.access" id="access_farm_animal_user">
        <field name="name">Farm Animal User</field>
        <field name="model_id" ref="model_animal"/>
        <field name="group_id" ref="base.group_user"/>
        <field name="perm_read" eval="1"/>
        <field name="perm_write" eval="0"/>
        <field name="perm_create" eval="0"/>
        <field name="perm_unlink" eval="0"/>
    </record>

    <record model="ir.model.access" id="access_farm_animal_manager">
        <field name="name">Farm Animal Manager</field>
        <field name="model_id" ref="model_animal"/>
        <field name="group_id" ref="farm_group_manager"/>
        <field name="perm_read" eval="1"/>
        <field name="perm_write" eval="1"/>
        <field name="perm_create" eval="1"/>
        <field name="perm_unlink" eval="1"/>
    </record>
